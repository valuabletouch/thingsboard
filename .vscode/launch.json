{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch ThingsBoard Installer",
            "request": "launch",
            "mainClass": "org.thingsboard.server.ThingsboardInstallApplication",
            "projectName": "application",
            "shortenCommandLine": "auto",
            "vmArgs": [
                "-Dloader.main=org.thingsboard.server.ThingsboardInstallApplication",
                "-Dinstall.data_dir=${workspaceFolder}/.vscode/launch/data",
                "-Dlogging.config=${workspaceFolder}/.vscode/launch/config/logback.xml",
                "-Xloggc:${workspaceFolder}/.vscode/launch/log/gc.log",
                "-Dinstall.load_demo=true",
                "-Dinstall.upgrade=false",
                "-XX:+IgnoreUnrecognizedVMOptions",
                "-XX:+HeapDumpOnOutOfMemoryError",
                "-XX:+PrintGCDetails",
                "-XX:+PrintGCDateStamps",
                "-XX:+PrintHeapAtGC",
                "-XX:+PrintTenuringDistribution",
                "-XX:+PrintGCApplicationStoppedTime",
                "-XX:+UseGCLogFileRotation",
                "-XX:NumberOfGCLogFiles=10",
                "-XX:GCLogFileSize=10M",
                "-XX:-UseBiasedLocking",
                "-XX:+UseTLAB",
                "-XX:+ResizeTLAB",
                "-XX:+PerfDisableSharedMem",
                "-XX:+UseCondCardMark",
                "-XX:CMSWaitDuration=10000",
                "-XX:+UseParNewGC",
                "-XX:+UseConcMarkSweepGC",
                "-XX:+CMSParallelRemarkEnabled",
                "-XX:+CMSParallelInitialMarkEnabled",
                "-XX:+CMSEdenChunksRecordAlways",
                "-XX:CMSInitiatingOccupancyFraction=75",
                "-XX:+UseCMSInitiatingOccupancyOnly",
                "-XX:+ExitOnOutOfMemoryError",
                "-Dspring.jpa.hibernate.ddl-auto=none",
            ],
            "args": [
                "org.springframework.boot.loader.PropertiesLauncher"
            ],
            "env": {
                "LOADER_PATH": "${workspaceFolder}/.vscode/launch/config",
                "LOG_FOLDER": "${workspaceFolder}/.vscode/launch/log",
                "LOG_FILENAME": "thingsboard",
                "SQL_DATA_FOLDER": "${workspaceFolder}/.vscode/launch/data/sql",
                "DATABASE_ENTITIES_TYPE": "sql",
                "DATABASE_TS_TYPE": "cassandra",
                "CASSANDRA_URL": "localhost:9042",
                "CASSANDRA_USE_CREDENTIALS": true,
                "CASSANDRA_USERNAME": "root",
                "CASSANDRA_PASSWORD": "secret",
                "SPRING_JPA_DATABASE_PLATFORM": "org.hibernate.dialect.PostgreSQLDialect",
                "SPRING_DRIVER_CLASS_NAME": "org.postgresql.Driver",
                "SPRING_DATASOURCE_URL": "jdbc:postgresql://localhost:5432/thingsboard",
                "SPRING_DATASOURCE_USERNAME": "root",
                "SPRING_DATASOURCE_PASSWORD": "secret",
                "READING_TYPE_MONGODB_URI": "mongodb://root:secret@127.0.0.1:27017/thingsboard?authSource=admin",
                "TRANSFORMATION_MONGODB_URI": "mongodb://root:secret@127.0.0.1:27017/thingsboard?authSource=admin",
                "TRANSFORMATON_SYSTEM_READING_TYPE":"thingsboard",
                "TRANSFORMATON_SYSTEM_THINGSBOARD":"thingsboard",
                "TRANSFORMATON_SYSTEM_TENANT":"tenant",
                "TRANSFORMATON_SYSTEM_DEVICE":"device",
                "TRANSFORMATON_SYSTEM_DATA_SOURCE":"datasource",
                "LOG_LEVEL": "info"
            }
        },
        {
            "type": "java",
            "name": "Launch ThingsBoard Migrator",
            "request": "launch",
            "mainClass": "org.thingsboard.client.tools.migrator.MigratorTool",
            "projectName": "tools",
            "env": {}
        },
        {
            "type": "java",
            "name": "Launch ThingsBoard Server",
            "request": "launch",
            "mainClass": "org.thingsboard.server.ThingsboardServerApplication",
            "projectName": "application",
            "shortenCommandLine": "jarmanifest",
            "vmArgs": [
                "-Dloader.main=org.thingsboard.server.ThingsboardServerApplication",
                "-Dlogging.config=${workspaceFolder}/.vscode/launch/config/logback.xml",
                "-Xloggc:${workspaceFolder}/.vscode/launch/log/gc.log",
                "-XX:+IgnoreUnrecognizedVMOptions",
                "-XX:+HeapDumpOnOutOfMemoryError",
                "-XX:+PrintGCDetails",
                "-XX:+PrintGCDateStamps",
                "-XX:+PrintHeapAtGC",
                "-XX:+PrintTenuringDistribution",
                "-XX:+PrintGCApplicationStoppedTime",
                "-XX:+UseGCLogFileRotation",
                "-XX:NumberOfGCLogFiles=10",
                "-XX:GCLogFileSize=10M",
                "-XX:-UseBiasedLocking",
                "-XX:+UseTLAB",
                "-XX:+ResizeTLAB",
                "-XX:+PerfDisableSharedMem",
                "-XX:+UseCondCardMark",
                "-XX:CMSWaitDuration=10000",
                "-XX:+UseParNewGC",
                "-XX:+UseConcMarkSweepGC",
                "-XX:+CMSParallelRemarkEnabled",
                "-XX:+CMSParallelInitialMarkEnabled",
                "-XX:+CMSEdenChunksRecordAlways",
                "-XX:CMSInitiatingOccupancyFraction=75",
                "-XX:+UseCMSInitiatingOccupancyOnly",
                "-XX:+ExitOnOutOfMemoryError",
                "-Dspring.jpa.hibernate.ddl-auto=none"
            ],
            "args": [
                "org.springframework.boot.loader.PropertiesLauncher"
            ],
            "env": {
                "LOADER_PATH": "${workspaceFolder}/.vscode/launch/config",
                "LOG_FOLDER": "${workspaceFolder}/.vscode/launch/log",
                "LOG_FILENAME": "thingsboard",
                "SQL_DATA_FOLDER": "${workspaceFolder}/.vscode/launch/data/sql",
                "TB_SERVICE_ID": "monolith",
                "TB_SERVICE_TYPE": "monolith",
                "ZOOKEEPER_ENABLED": true,
                "ZOOKEEPER_URL": "localhost:2181",
                "JS_EVALUATOR": "local",
                "TRANSPORT_TYPE": "local",
                "CACHE_TYPE": "redis",
                "REDIS_HOST": "localhost",
                "REDIS_PORT": 6379,
                "REDIS_DB": 1,
                "REDIS_PASSWORD": "secret",
                "DATABASE_ENTITIES_TYPE": "sql",
                "DATABASE_TS_TYPE": "cassandra",
                "CASSANDRA_USE_VSENSOR": true,
                "CASSANDRA_URL": "localhost:9042",
                "CASSANDRA_USE_CREDENTIALS": true,
                "CASSANDRA_KEYSPACE_NAME_VSENSOR": "thingsboard",
                "CASSANDRA_USERNAME": "root",
                "CASSANDRA_PASSWORD": "secret",
                "SPRING_JPA_DATABASE_PLATFORM": "org.hibernate.dialect.PostgreSQLDialect",
                "SPRING_DRIVER_CLASS_NAME": "org.postgresql.Driver",
                "SPRING_DATASOURCE_URL": "jdbc:postgresql://localhost:5432/thingsboard",
                "SPRING_DATASOURCE_USERNAME": "root",
                "SPRING_DATASOURCE_PASSWORD": "secret",
                "READING_TYPE_MONGODB_URI": "mongodb://root:secret@127.0.0.1:27017/thingsboard?authSource=admin",
                "TRANSFORMATION_MONGODB_URI": "mongodb://root:secret@127.0.0.1:27017/thingsboard?authSource=admin",
                "TRANSFORMATON_SYSTEM_READING_TYPE":"thingsboard",
                "TRANSFORMATON_SYSTEM_THINGSBOARD":"thingsboard",
                "TRANSFORMATON_SYSTEM_TENANT":"tenant",
                "TRANSFORMATON_SYSTEM_DEVICE":"device",
                "TRANSFORMATON_SYSTEM_DATA_SOURCE":"datasource",
                "TB_QUEUE_TYPE": "rabbitmq",
                "TB_QUEUE_RABBIT_MQ_USERNAME": "root",
                "TB_QUEUE_RABBIT_MQ_PASSWORD": "secret",
                "TB_QUEUE_RABBIT_MQ_HOST": "localhost",
                "TB_QUEUE_RABBIT_MQ_PORT": 5672,
                "TB_QUEUE_PARTITIONS_VIRTUAL_NODES_SIZE": 64,
                "REMOTE_JS_EVAL_REQUEST_TOPIC": "js_eval.requests",
                "REMOTE_JS_EVAL_RESPONSE_TOPIC": "js_eval.responses",
                "HTTP_BIND_PORT": 5100,
                "MQTT_BIND_PORT": 11883,
                "HTTP_LOG_CONTROLLER_ERROR_STACK_TRACE": false,
                "LOG_LEVEL": "info",
                "JWT_TOKEN_ISSUER": "v-sensor",
                "JWT_TOKEN_SIGNING_KEY": "c2VjcmV0", // "secret" Base64 encoded
                "JWT_TOKEN_EXPIRATION_TIME": 36000,
                "JWT_REFRESH_TOKEN_EXPIRATION_TIME": 1728000,
                "SECURITY_USER_TOKEN_ACCESS_ENABLED": true,
                "SECURITY_USER_LOGIN_CASE_SENSITIVE": true,
                "METRICS_ENABLED": true,
                "METRICS_ENDPOINTS_EXPOSE": "prometheus"
            }
        },
        {
            "type": "java",
            "name": "Launch ThingsBoard Coap Transport Server",
            "request": "launch",
            "mainClass": "org.thingsboard.server.coap.ThingsboardCoapTransportApplication",
            "projectName": "coap"
        },
        {
            "type": "java",
            "name": "Launch ThingsBoard Http Transport Server",
            "request": "launch",
            "mainClass": "org.thingsboard.server.http.ThingsboardHttpTransportApplication",
            "projectName": "http"
        },
        {
            "type": "java",
            "name": "Launch ThingsBoard Mqtt Transport Server",
            "request": "launch",
            "mainClass": "org.thingsboard.server.mqtt.ThingsboardMqttTransportApplication",
            "projectName": "application"
        },
        {
            "type": "java",
            "name": "Launch Coap Device Emulator",
            "request": "launch",
            "mainClass": "org.thingsboard.server.transport.coap.client.DeviceEmulator",
            "projectName": "coap"
        },
        {
            "type": "java",
            "name": "Debug Mqtt Ssl Client",
            "request": "launch",
            "mainClass": "org.thingsboard.client.tools.MqttSslClient",
            "projectName": "tools"
        }
    ]
}